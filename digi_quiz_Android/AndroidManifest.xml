<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.digitalMedia.quiz"
    android:versionCode="1"
    android:versionName="1.0" >

     <uses-sdk android:minSdkVersion="7" android:targetSdkVersion="17"/>

    <application
        android:label="@string/app_name"
        android:theme="@style/AppTheme" android:allowBackup="false" android:icon="@drawable/ic_launcher">
        <activity
            android:name="EnterGameActivity"
            android:label="@string/title_activity_enter_game" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.digitalMedia.quiz.QnA_Activity"
            android:label="@string/title_activity_game_options" >
        </activity>
        <activity
            android:name="QuizGameActivity"
            android:label="@string/app_name" >
        </activity>
    </application>

</manifest>